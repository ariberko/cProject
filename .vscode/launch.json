{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run preprocessor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/preprocessor",
            "args": ["input.as"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build preprocessor",
            "miDebuggerPath": "/workspaces/cProject"
        }
    ]
}
